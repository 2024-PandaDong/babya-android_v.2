        <action
            android:id="@+id/action_editDiaryFragment_to_diaryFragment"
            app:destination="@id/diaryFragment" />
    </fragment>
    <fragment
        android:id="@+id/editDashBoardFragment"
        android:name="kr.pandadong2024.babya.home.dash_board.EditDashBoardFragment"
